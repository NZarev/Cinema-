import java.util.Scanner;

public class Cinema {
    public static void main(String[] args) {
        Scanner scanner = new Scanner(System.in);

        String projectionType = scanner.nextLine();
        int rows = scanner.nextInt();
        int columns = scanner.nextInt();

        double price = 0.0;

        switch (projectionType) {
            case "Premiere":
                price = 12.00;
                break;
            case "Normal":
                price = 7.50;
                break;
            case "Discount":
                price = 5.00;
                break;
            default:
                System.out.println("Invalid projection type");
                return;
        }

        double totalRevenue = rows * columns * price;
        System.out.printf("%.2f leva%n", totalRevenue);

        scanner.close();
    }
}
